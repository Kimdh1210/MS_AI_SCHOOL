    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy import stats\n",
    "\n",
    "# 이항분포의 주요 파라미터는 2개로,\n",
    "# 시행 횟수 n과 해당 시행의 각 성공 확률 p로 이루어짐\n",
    "n = 10\n",
    "p = 0.5\n",
    "\n",
    "num_simulation = 1000\n",
    "success = np.random.binomial(n, p, num_simulation)\n",
    "\n",
    "plt.hist(success, bins=10, range=(0, 10), alpha=0.5)\n",
    "plt.show()"
   ]
